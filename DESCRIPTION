    R (>= 3.0.0)
Imports:
    methods,
    grid,
    stats,
    assertthat (>= 0.1),
    gridExtra (>= 2.0.0),
    reshape2 (>= 1.4.1),
    ggplot2 (>= 2.0.0)
Enhances: 
    pbdPAPI (>= 0.2-2),
    pbdPROF (>= 0.4-0)
Suggests:
    knitr,
    rmarkdown
VignetteBuilder: knitr
NeedsCompilation: no
ByteCompile: yes
